{
  "name": "user-data-for-fraud-prevention",
  "version": "1.11.1",
  "description": "Detects Timezone, screen size, scaling factor, browser plugins, local device IPs etc;",
  "scripts": {
    "test": "jest",
    "build": "webpack",
    "test:coverage": "npm test -- --coverage && cat ./coverage/lcov.info | coveralls",
    "lint:fix": "eslint '{src,tests}/**/*.{js,ts} --fix",
    "lint": "eslint '{src,tests}/**/*.{js,ts}'",
    "format": "lint-staged",
    "prettier": "prettier '{src,tests}/**/*.{js,ts}' --config .prettierrc --write",
    "prettier:fix": "prettier '{src,tests}/**/*.{js,ts}' --config .prettierrc",
    "clean": "rm -rf node_modules",
    "rebuild": "npm run clean && npm install && npm run build",
    "prepublishOnly": "webpack"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.ts": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/intuit/user-data-for-fraud-prevention.git"
  },
  "main": "dist/bundle.js",
  "keywords": [
    "Fraud",
    "Prevention",
    "Headers"
  ],
  "author": "skodamarthi",
  "license": "ISC",
  "files": [
    "src/js/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@semantic-release/changelog": "^6.0.0",
    "@semantic-release/commit-analyzer": "^9.0.1",
    "@semantic-release/git": "^10.0.0",
    "@semantic-release/npm": "^10.0.0",
    "@semantic-release/release-notes-generator": "^10.0.2",
    "@types/jest": "^29.0.0",
    "@typescript-eslint/eslint-plugin": "5.2.0",
    "@typescript-eslint/parser": "5.2.0",
    "all-contributors-cli": "^6.17.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.0.0",
    "coveralls": "^3.1.0",
    "eslint": "^8.0.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^27.0.0",
    "husky": "^8.0.0",
    "jest": "^29.0.0",
    "jest-cli": "^29.0.0",
    "jest-coverage-badges": "^1.1.2",
    "lint-staged": "^13.0.0",
    "prettier": "^2.0.5",
    "react-addons-test-utils": "^15.6.2",
    "semantic-release": "^18.0.0",
    "ts-jest": "^29.0.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.0.0",
    "webpack": "^5.0.0",
    "webpack-cli": "^5.0.0"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tx,tsx}",
      "!**/test/**"
    ]
  },
  "directories": {
    "test": "tests"
  }
}
